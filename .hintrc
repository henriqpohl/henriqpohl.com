{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "font-synthesis",
          "scroll-behavior",
          "@layer"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not chrome <= 112",
    "not edge <= 111",
    "not opera <= 95",
    "not samsung <= 20"
  ]
}